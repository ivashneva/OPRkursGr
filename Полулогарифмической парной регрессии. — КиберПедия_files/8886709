  let globalCommunityArr = globalCommunityLists.reverse()
  for (let i = 0; i < globalCommunityArr.length; i++) {
    const theOne = globalCommunityArr[i];
    let $dom = $(`
      <div class="community-icon-item">
        ${theOne.icon}
        <ul>
          <i></i>
        </ul>
      </div>
    `)
    for (let j = 0; j < theOne.lists.length; j++) {
      const theOneLi = theOne.lists[j];
      let $li = $(`
        <li>
          <a href="${theOneLi.website}"> ${theOneLi.lang} </a>
        </li>
      `)
      $dom.find('ul').append($li)
    }
    if (theOne.lists.length < 1) {
      let $li = $(`
        <li>
          <a href="${theOne.link}">English</a>
        </li>
      `)
      $dom.find('ul').css('width', '80px')
      $dom.find('ul').append($li)
    }
    $('.gateio-community-icon-container').prepend($dom)
  }

  function JumpTo(url) {
    window.location.href = url
  }
