            <script>
              function conversionNumberAndUnit (num) {
                if(!num || num === "" || typeof num === 'undefined') return { num:"0.00", unit:""};
                num = num.replace(/,/g,'')
                var unit = ""
                num = new Number(num);
                if (g_lang === 'cn') {
                  unit = "美金"
                  if (num > 100000000) {
                    num = (num/100000000).toFixed(2)
                    unit = '亿美金'
                  } else if (num > 10000) {
                    num = (num/10000).toFixed(2)
                    unit = '万美金'
                  } else if (num < -100000000) {
                    num = (num/100000000).toFixed(2)
                    unit = '亿美金'
                  } else if (num < -10000) {
                    num = (num/10000).toFixed(2)
                    unit = '万美金'
                  } else {
                    num = num.toFixed(2);
                  }
                } else {
                  if(num > 1000000000){
                    num = (num/1000000000).toFixed(2)
                    unit = 'B'
                  } else if (num > 1000000) {
                    num = (num/1000000).toFixed(2)
                    unit = 'M'
                  } else if (num > 1000) {
                    num = (num/1000).toFixed(2)
                    unit = 'K'
                  } else if (num < -1000000000){
                    num = (num/1000000000).toFixed(2)
                    unit = 'B'
                  } else if (num < -1000000) {
                    num = (num/1000000).toFixed(2)
                    unit = 'M'
                  } else if (num < -1000) {
                    num = (num/1000).toFixed(2)
                    unit = 'K'
                  } else {
                    num = num.toFixed(2);
                  }
                }
                return { num:num,unit: unit };
              }
              try {
                let quantdata = JSON.parse('{"num":956890,"total_follow_num":106605,"total_invest":"1606270998.124783536154924","copy_profit":"1725373.200239703658162655","profit_share":"88063.4233016905923985018","unsettled_share":"6866.42809884500919778","ranking_update_time":1639273834,"max_profit_rate":"14.010176583","num_change":2829,"follow_num_change":597,"max_profit_rate_change":"0.420561166"}')
                let spotNumber = '1,103,896,472'
                let contractNumber = '1,451,196,000'
                let spotNumberAndUnit = conversionNumberAndUnit(spotNumber)
                let contractNumberAndUnit = conversionNumberAndUnit(contractNumber)
                $('#usdt_all_vol').html('$'+spotNumberAndUnit.num+'<span class="main-top-dashboard-item-unit">'+spotNumberAndUnit.unit+'</span>')
                $('#contract_all_vol').html('$'+contractNumberAndUnit.num+'<span class="main-top-dashboard-item-unit">'+contractNumberAndUnit.unit+'</span>')
                if (quantdata.num) {
                  let total_invest = conversionNumberAndUnit(formatToLocalCurrency(quantdata.total_invest, { maximumFractionDigits: 0 }))
                  let max_profit_rate = quantdata.max_profit_rate;
                  $('#quant-fund-num').html('$' +total_invest.num+'<span class="main-top-dashboard-item-unit">'+total_invest.unit +'</span>')
                  $('#quant-profit').text(max_profit_rate?Math.round(max_profit_rate * 100) + '%':"--")
                  $('#quant-num-change').text((quantdata.num_change >= 0 ? '+' : '') + quantdata.num_change).addClass(quantdata.num_change >= 0 ? 'red' : 'green')
                  $('#quant-copy-change').text((quantdata.follow_num_change >= 0 ? '+' : '') + quantdata.follow_num_change).addClass(quantdata.follow_num_change >= 0 ? 'red' : 'green')
                }
              } catch (error) {
                console.log(error)
              }
            </script>
        </div>
      </div>
          </div>
  </section>
