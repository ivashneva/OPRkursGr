    $(document).ready(function () {

        if ($("div").is("#banner")) {
            $('#banner').append('<button id="close-btn" block-id="banner">x</button>');
            $('#banner').append('<div id="logoParade"></div>');

            for (var n = 0; n < 6; n++) {
                $('#logoParade').append('<a onclick="newpage();"><img src="/upload/medialibrary/66e/66eb982a857f27e31a74b0166a25ca30.jpg"  border="0"/></a>');
            }

            $("#logoParade").smoothDivScroll({
                autoScrollingMode: "onStart",
                autoScrollingDirection: "endlessLoopRight",
                autoScrollingStep: 1,
                autoScrollingInterval: 15
            });

            $("#logoParade").bind("mouseover", function () {
                $(this).smoothDivScroll("stopAutoScrolling");
            }).bind("mouseout", function () {
                $(this).smoothDivScroll("startAutoScrolling");
            });
        }


        $(".notificator-close , .notificator-link").on("click", function () {
