        function addBasketJTM(list,productID){

            // old dataLayer.push
            return;

            var product=eccomerce_json_catalog[productID];

            window.dataLayer = window.dataLayer || [];
            dataLayer.push({
                "event": "addToCart",
                "ecommerce": {
                    "currencyCode": "USD",
                    "add": {
                        "actionField": {
							"list": list
                        },
                        "products": [product]
                    }
                }
            });
        }

		function productClickGTM(list,productID,link){

            // old dataLayer.push
            return;

			var product=eccomerce_json_catalog_no_qnt[productID];

			window.dataLayer = window.dataLayer || [];
			dataLayer.push({
				"event": "productClick",
				'ecommerce': {
					'currencyCode': 'USD',
					'click': {
						'actionField': {'list': list},
						'products': [product]
					}
				},
			   'eventCallback': function () {
				   document.location = link;
			   }

			});
		}

        function addMoreJTM(list,productID){

            // old dataLayer.push
            return;

            var product=eccomerce_json_catalog_no_qnt[productID];

            window.dataLayer = window.dataLayer || [];
            dataLayer.push({
                "event": "productClick",
                'ecommerce': {
                    'currencyCode': 'USD',
                    'click': {
                        'actionField': {'list': list},
                        'products': [product]
                    }
                }
            });
        }
		
        function addQuickJTM(list,productID){

            // old dataLayer.push
            return;

            var product=eccomerce_json_catalog_no_qnt[productID];

            window.dataLayer = window.dataLayer || [];
            dataLayer.push({
                "event":"quickView",
                "ecommerce": {
                    "detail": {
                        "actionField": {
                            "list": list
                        },
                        "products": [product]
                    },
                }
            });

            window.dataLayer = window.dataLayer || [];
            dataLayer.push({
                "event": "productClick",
                'ecommerce': {
                    'currencyCode': 'USD',
                    'click': {
                        'actionField': {'list': list},
                        'products': [product]
                    }
                }
            });

        }




    </script>

    

















  
