		<script data-skip-moving="true" >
    let omaParams = {
        delivery : {
            is_delivery : true,
		        has_free_delivery : true,
            total : 0,
            zone : 1,
		        type : "S",
		        type_cart : "S",
            delivery_cost : 0,
            products_params : {
                weight : 0,
                volume : 0,
                max_size : 0
            },
            fs_threshold : 0,
            total_threshold : 0
        },
        disableCartPopupTimeout : false,
        basket : [],
        setBasket(products){
            this.basket = products;
        },
        addProductBasket(product){
            this.basket.push(product);
        },
        getBasket(){
            return this.basket;
        },
        isProductInBasket(productId){
            let basket = this.getBasket();

            if(basket.length>0){
                let productArr = this.basket.filter(function(product) {
                    return Number(product['product_id']) == Number(productId);
                });

                if(productArr.length>0){
                    let product = productArr.shift();
                    return {
                        status : true,
                        quantity : product['quantity']
                    }
                }
            }

            return {
                status : false,
                quantity : null
            }

        },
    };
    omaParams.setBasket([]);
</script>

    
	    
