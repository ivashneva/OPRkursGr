<script type="text/javascript">
      function withdradio(p,ex,fee_k,fee_f) {
        var b = $(p).val();
        $('#window_withdrawal input[name=fee_k]').val(fee_k);
        $('#window_withdrawal input[name=fee_fix]').val(fee_f);
        $('#window_withdrawal input[name=amount]').keyup();
        $('#window_withdrawal input[name=fiat_service]').val(b);
        $("#window_withdrawal .wallet_example span").html(ex);
        $("#window_withdrawal .billcont").hide();
        $("#window_withdrawal #cont"+b).show();
        if(b == 4) // advcash
        {
          $('#window_withdrawal .att').html("<span style='color:blue !important'>Внимание! Для вывода суммы более 7000 руб - вам нужна верификация в аккаунте Advcash!<br>Без авторизации в Advcash, такие выводы будут отменяться!</span>");
        }
        else
        {
          $('#window_withdrawal .att').html('');
        }
      }
    </script> <div class="window" id="window_deposit">
