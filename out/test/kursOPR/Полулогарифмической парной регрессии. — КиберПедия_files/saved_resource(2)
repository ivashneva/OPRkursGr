<script type="text/javascript">
   $(document).ready(function(){
      $('.mobile-img').slick({
         autoplay: true
      });

      let typeMobile = checkMobile(navigator.userAgent);
      if(typeMobile == 'android' || typeMobile == 'ios') {
        $( ".app-mobile" ).addClass('open');
        $(".app-mobile [data-mobile]").hide();
        $('.app-mobile [data-mobile][data-typeMobile="'+typeMobile+'"]').show();
      }
      
      $('.app-mobile .mc-popup').click( function(event) {
         $('.app-mobile').removeClass('open');
      });
   });
</script>
