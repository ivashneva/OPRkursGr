function pre(e)
{
	console.log(e);
}

function onTradeLastPrice(req)
{
	var arData = req['data'];
	var arPair = arData['pair'].split('_');


	if ($('.currency-card-' + arData['pair']).length)
	{
		//pre('slider'); pre(arData);

		var se = $('.currency-card-' + arData['pair']);

		var delta = $(se).data('delta');
		var pricePrec = $(se).data('prec');
		//pre([delta, arData['delta2']]);

		if (delta != arData['delta2'])
		{
			//pre([delta, arData['delta2']]);

			$(se).data('delta', arData['delta2']);
			se.find('.currency-card-diff-number').removeClass('currency-card-diff-number--negative currency-card-diff-number--positive');

			if (arData['delta2'] == 'down')
			{
				se.find('.currency-card-diff-number').addClass('currency-card-diff-number--negative');

				if (typeof arMiniCharts[arData['pair']] != 'undefined')
				{
					arMiniCharts[arData['pair']].stroke('2 #F0937C');
					arMiniCharts[arData['pair']].fill(['#F0937C 0.2', '#F0937C 0.01'], -90, true);

					$('.chart-mini-' + arData['pair']).html($('#chart_mini_' + arData['pair']).html());
				}
			}
			else
			{
				se.find('.currency-card-diff-number').addClass('currency-card-diff-number--positive');

				if (typeof arMiniCharts[arData['pair']] != 'undefined')
				{
					arMiniCharts[arData['pair']].stroke('2 #3FD027');
					arMiniCharts[arData['pair']].fill(['#3FD027 0.2', '#3FD027 0.01'], -90, true);

					$('.chart-mini-' + arData['pair']).html($('#chart_mini_' + arData['pair']).html());
				}
			}
		}

		se.find('.currency-card-price').html('<i class="fa fa-' + arPair[1].toLowerCase() + '"></i>'+tradeAmountFormat(number_format(arData['price'], pricePrec, '.', '')));
		se.find('.currency-card-diff-percent span').html(number_format(Math.abs(arData['delta']), 2, '.', ','));
		se.find('.currency-card-diff-number').html((arData['delta2'] == 'up' ? '+' : '-') + '<i class="fa fa-' + arPair[1].toLowerCase() + '"></i>' + tradeAmountFormat(number_format(arData['delta_price'], pricePrec, '.', '')));
		se.find('.currency-card-total').html(number_format(arData['amount'], arCurrPr[arPair[0]], '.', ','));
	}


	if (arPair[1] == 'USD' && $('.trading-stats__item-block-' + arPair[0]).length)
	{
		//pre('main'); pre(arData);

		var e = $('.trading-stats__item-block-' + arPair[0]);

		var delta = $(e).data('delta');
		var pricePrec = $(e).data('prec');
		//pre([delta, arData['delta2']]);

		if (delta != arData['delta2'])
		{
			$(e).data('delta', arData['delta2']);

			e.find('.trading-stats__item-change').removeClass('trading-stats__item-change_down');

			if (arData['delta2'] == 'down')
			{
				e.find('.trading-stats__item-change').addClass('trading-stats__item-change_down');

				if (typeof arCharts[arPair[0]] != 'undefined')
				{
					arCharts[arPair[0]].stroke('2 #F0937C');
					arCharts[arPair[0]].fill(['#F0937C 0.2', '#F0937C 0.01'], -90, true);
				}
			}
			else
			{
				if (typeof arCharts[arPair[0]] != 'undefined')
				{
					arCharts[arPair[0]].stroke('2 #3FD027');
					arCharts[arPair[0]].fill(['#3FD027 0.2', '#3FD027 0.01'], -90, true);
				}
			}
		}

		var amount = e.find('.trading-stats__item-order').data('amount');
		//pre([amount, arData['amount']]);
		e.find('.trading-stats__item-order svg').removeClass('hide');

		if (arData['amount']*1 >= amount*1)
		{
			e.find('.trading-stats__item-order svg.trading-stats__item-order-arrow-down').addClass('hide');
		}
		else
		{
			e.find('.trading-stats__item-order svg.trading-stats__item-order-arrow-up').addClass('hide');
		}

		e.find('.trading-stats__item-price').html(tradeAmountFormat(number_format(arData['price'], pricePrec, '.', '')));
		e.find('.trading-stats__item-change_percent').html((arData['delta2'] == 'up' ? '+' : '-') + number_format(Math.abs(arData['delta']), 2, '.', ',') + '<span class="insgnzrs">%</span>');
		e.find('.trading-stats__item-change_main').html(tradeAmountFormat(number_format(arData['delta_price'], pricePrec, '.', '')));
		e.find('.trading-stats__item-order-size').html(tradeAmountFormat(number_format(arData['amount'], arCurrPr[arPair[0]], '.', '')));
	}
}
</script>

