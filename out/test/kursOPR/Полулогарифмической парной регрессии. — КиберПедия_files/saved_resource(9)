					offerBumperHeight = offerBumperPlaceholder.offsetHeight;
				} else { //offer bumper is loaded
					var offerBumperContentList = document.querySelectorAll('.offer-bumper-content');
					offerBumperHeight = offerBumperContentList.length > 0 ? Math.max(...Array.from(offerBumperContentList).map((el) => {return el['offsetHeight']})) : 0;
				}

				var navigationHeight = navigationWrapperHeight + offerBumperHeight - offerBumperToggleHeight;
				document.querySelector('#wrapper').style.paddingTop = navigationHeight + 'px';
			}
