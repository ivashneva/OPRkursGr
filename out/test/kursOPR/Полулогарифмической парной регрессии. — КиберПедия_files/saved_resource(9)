			if (document.readyState === 'interactive') {
				var navigationWrapper = document.querySelector('i-navigation-wrapper');
				var navigationWrapperHeight = navigationWrapper ? navigationWrapper.offsetHeight : 0;
				var offerBumperToggle = document.querySelector('.offer-bumper-toggle');
