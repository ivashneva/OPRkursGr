<script>
        



if ('serviceWorker' in navigator) {
if (true) {
window.addEventListener('load', function() {
navigator.serviceWorker.register('/service-worker.js', {scope: '/'}).then(function(success) {
if (false) {
console.log('Successfully registered service worker', success);
}
}).catch(function(error) {
if (false) {
console.log('Error while registering service worker', error.message);
}
});
});
} else {
navigator.serviceWorker.getRegistrations().then(function(registrations) {
registrations.forEach(function(registration) {
if (registration.active && registration.active.scriptURL.match(/service-worker.js/)) {
registration.unregister();
}
});
});
}
}

    </script>
